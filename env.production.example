# Auction Client - Production Environment Variables
# Copy this to .env.production and fill in your actual values

# API URLs (Replace with your actual Render deployment URLs)
REACT_APP_UNIFIED_AUTH_URL=https://your-auth-service.onrender.com
REACT_APP_AUCTION_SERVER_URL=https://your-auction-server.onrender.com
REACT_APP_CLIENT_URL=https://your-auction-client.vercel.app
REACT_APP_MAIN_SITE_URL=https://your-pulasa-main.vercel.app

# Razorpay Configuration (Required for bidding)
REACT_APP_RAZORPAY_KEY_ID=your-razorpay-key-id-here
REACT_APP_RAZORPAY_KEY_SECRET=your-razorpay-key-secret-here

# Optional: Analytics and monitoring
REACT_APP_ENV=production
REACT_APP_VERSION=1.0.0 